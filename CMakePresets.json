                "ENABLE_SDKLIB_EXAMPLES": "ON",
                "ENABLE_SDKLIB_TESTS": "ON",
                "ENABLE_SDKLIB_WERROR": "ON",
                "ENABLE_SYNC": "ON",
                "ENABLE_TSAN": "OFF",
                "ENABLE_UBSAN": "OFF"
            }
        },
        {
            "name": "dev-unix",
            "inherits": [
                "unix",
                "dev"
            ]
        },
        {
            "name": "dev-windows",
            "inherits": [
                "windows",
                "dev"
            ]
        }
