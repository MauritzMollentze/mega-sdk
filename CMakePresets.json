{
    "version": 2,
    "configurePresets": [
        {
            "name": "default",
            "hidden": true,
            "binaryDir": "${sourceParentDir}/build-${sourceDirName}-${presetName}",
            "cacheVariables": {
                "VCPKG_ROOT": "${sourceParentDir}/vcpkg"
            }
        },
        {
            "name": "unix",
            "hidden": true,
            "generator": "Unix Makefiles"
        },
        {
            "name": "android",
            "hidden": true,
            "inherits": "unix",
            "cacheVariables": {
                "CMAKE_SYSTEM_NAME": "Android"
            }
        },
        {
            "name": "ios",
            "hidden": true,
            "inherits": "unix",
            "cacheVariables": {
                "CMAKE_SYSTEM_NAME": "iOS"
            }
        },
        {
            "name": "windows",
            "hidden": true,
