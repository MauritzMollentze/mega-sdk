        },
        {
            "name": "megacmd-unix",
            "inherits": [
                "unix",
                "megacmd"
            ]
        },
        {
            "name": "megacmd-windows",
            "inherits": [
                "windows",
                "megacmd"
            ]
        },
        {
            "name": "megaproxy",
            "hidden": true,
            "inherits": "default",
            "cacheVariables": {
                "ENABLE_ISOLATED_GFX": "OFF",
                "ENABLE_LOG_PERFORMANCE": "ON",
                "ENABLE_SYNC": "OFF",
                "USE_LIBUV": "OFF",
                "USE_READLINE": "OFF"
            }
        },
        {
            "name": "megaproxy-unix",
            "inherits": [
                "unix",
                "megaproxy"
            ]
        },
        {
            "name": "megasync",
            "hidden": true,
            "inherits": "default",
            "cacheVariables": {
                "ENABLE_ISOLATED_GFX": "ON",
                "ENABLE_LOG_PERFORMANCE": "ON",
                "ENABLE_QT_BINDINGS": "ON",
                "USE_LIBUV": "ON"
            }
        },
        {
            "name": "megasync-unix",
            "inherits": [
                "unix",
                "megasync"
            ]
        },
        {
            "name": "megasync-windows",
            "inherits": [
                "windows",
                "megasync"
            ]
        },
        {
            "name": "mega-vpn",
            "hidden": true,
            "inherits": "default"
        },
        {
            "name": "mega-vpn-android",
            "inherits": [
                "android",
                "mega-vpn"
            ],
            "cacheVariables": {
                "ENABLE_CHAT": "OFF",
                "ENABLE_SYNC": "OFF",
                "USE_LIBUV": "OFF"
            }
        },
        {
            "name": "dev",
            "hidden": true,
            "inherits": "default",
